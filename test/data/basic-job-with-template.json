{
    "annotations": {},
    "jobs": {
        "main": [{
            "annotations": {},
            "image": "node:4",
            "commands": [
                {
                    "name": "install",
                    "command": "npm install"
                },
                {
                    "name": "test",
                    "command": "npm test"
                }
            ],
            "environment": {
                "FOO": "overwritten by job",
                "BAR": "foo"
            },
            "secrets": [
                "GIT_KEY"
            ],
            "settings": {
                "email": "foo@example.com"
            },
            "requires": [
                "~pr",
                "~commit"
            ]
        }],
      "publish": [{
          "annotations": {},
          "image": "node:4",
          "commands": [
              {
                  "name": "publish",
                  "command": "npm publish"
              }
          ],
          "environment": {},
          "secrets": [],
          "settings": {},
          "requires": [
              "main"
          ]
      }]
    },
    "workflowGraph": {
        "nodes": [
            { "name": "~pr" },
            { "name": "~commit" },
            { "name": "main" },
            { "name": "publish" }
        ],
        "edges": [
            { "src": "~pr", "dest": "main" },
            { "src": "~commit", "dest": "main" },
            { "src": "main", "dest": "publish" }
        ]
    }
}
